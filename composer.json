{
    "name": "kusabi/container",
    "description": "A PSR-11 conforming container library for PHP",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Christian Harvey",
            "email": "kusabi.software@gmail.com"
        }
    ],
    "keywords": [
        "container",
        "dependency",
        "di",
        "injection",
        "provider",
        "service"
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.4",
        "phan/phan": "^1.2",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Kusabi\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kusabi\\Container\\Tests\\": "tests/"
        }
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    }
}
